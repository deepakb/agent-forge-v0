# LLM Provider API Keys
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Logging
LOG_LEVEL=info

# Agent Configuration
METRICS_INTERVAL_MS=60000
CPU_THRESHOLD=0.8
MEMORY_THRESHOLD=0.85
ERROR_RATE_THRESHOLD=0.1
RESPONSE_TIME_THRESHOLD=5000

# Content Generation
DEFAULT_CONTENT_MODEL=gpt-4
FALLBACK_CONTENT_MODEL=claude-2
MAX_RETRIES=3
RETRY_DELAY_MS=1000

# Publishing
RATE_LIMIT_WEBSITE=30
RATE_LIMIT_BLOG=20
RATE_LIMIT_SOCIAL=10
RATE_LIMIT_EMAIL=5
RATE_LIMIT_DOCS=15
